<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainFragment">

<!--MainFragment-->

    <fragment
        android:id="@+id/MainFragment"
        android:name="edu.gtri.gpssample.fragments.main.MainFragment"
        tools:layout="@layout/fragment_main">

        <action
            android:id="@+id/action_navigate_to_SignUpFragment"
            app:destination="@id/SignUpFragment" />

        <action
            android:id="@+id/action_navigate_to_SignInFragment"
            app:destination="@id/SignInFragment" />
    </fragment>

    <!--SignUpFragment-->

    <fragment
        android:id="@+id/SignUpFragment"
        android:name="edu.gtri.gpssample.fragments.sign_up.SignUpFragment"
        tools:layout="@layout/fragment_sign_up">

        <action
            android:id="@+id/action_navigate_to_SignInFragment"
            app:destination="@id/SignInFragment" />
    </fragment>

    <!--SignInFragment-->

    <fragment
        android:id="@+id/SignInFragment"
        android:name="edu.gtri.gpssample.fragments.sign_in.SignInFragment"
        tools:layout="@layout/fragment_sign_in">

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:destination="@id/ManageConfigurationsFragment" />

        <action
            android:id="@+id/action_navigate_to_EnumeratorFragment"
            app:destination="@id/EnumeratorFragment" />
    </fragment>

    <!-- EnumeratorFragment   -->

    <fragment
        android:id="@+id/EnumeratorFragment"
        android:name="edu.gtri.gpssample.fragments.enumerator.EnumeratorFragment"
        tools:layout="@layout/fragment_enumerator">

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:destination="@id/ManageConfigurationsFragment" />

        <action
            android:id="@+id/action_navigate_to_SystemStatusFragment"
            app:destination="@id/SystemStatusFragment" />
    </fragment>

    <!--ManageConfigurationsFragment-->

    <fragment
        android:id="@+id/ManageConfigurationsFragment"
        android:name="edu.gtri.gpssample.fragments.manageconfigurations.ManageConfigurationsFragment"
        tools:layout="@layout/fragment_manage_configurations">

        <action
            android:id="@+id/action_navigate_to_CreateConfigurationFragment"
            app:destination="@id/CreateConfigurationFragment"/>

        <action
            android:id="@+id/action_navigate_to_ConfigurationFragment"
            app:destination="@id/ConfigurationFragment" />

        <action
            android:id="@+id/action_navigate_to_PerformEnumerationFragment"
            app:destination="@id/PerformEnumerationFragment" />

        <action
            android:id="@+id/action_navigate_to_NetworkConnectionDialogFragment"
            app:destination="@id/NetworkConnectionDialogFragment" />

        <action
            android:id="@+id/action_navigate_to_ClientFragment"
            app:destination="@id/ClientFragment" />
    </fragment>

    <!--CreateConfigurationFragment-->

    <fragment
        android:id="@+id/ConfigurationFragment"
        android:name="edu.gtri.gpssample.fragments.configuration.ConfigurationFragment"
        tools:layout="@layout/fragment_configuration">

        <action
            android:id="@+id/action_navigate_to_CreateConfigurationFragment"
            app:destination="@id/CreateConfigurationFragment"/>
        <action
            android:id="@+id/action_navigate_to_DefineEnumerationAreaFragment"
            app:destination="@id/DefineEnumerationAreaFragment"/>
        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:destination="@id/ManageConfigurationsFragment" />
        <action
            android:id="@+id/action_navigate_to_CreateStudyFragment"
            app:destination="@id/CreateStudyFragment" />
        <action
            android:id="@+id/action_navigate_to_ManageEnumerationAreaFragment"
            app:destination="@id/ManageEnumerationAreaFragment" />
        <action
            android:id="@+id/action_navigate_to_NetworkConnectionDialogFragment"
            app:destination="@id/NetworkConnectionDialogFragment" />


    </fragment>

    <!--CreateConfigurationFragment-->

    <fragment
        android:id="@+id/CreateConfigurationFragment"
        android:name="edu.gtri.gpssample.fragments.createconfiguration.CreateConfigurationFragment"
        tools:layout="@layout/fragment_create_configuration">

        <action
            android:id="@+id/action_navigate_to_DefineEnumerationAreaFragment"
            app:destination="@id/DefineEnumerationAreaFragment"/>
        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:destination="@id/ManageConfigurationsFragment" />
        <action
            android:id="@+id/action_navigate_to_CreateStudyFragment"
            app:destination="@id/CreateStudyFragment" />
    </fragment>

    <!--DefineEnumerationAreaFragment-->

    <fragment
        android:id="@+id/DefineEnumerationAreaFragment"
        android:name="edu.gtri.gpssample.fragments.define_enumeration_area.DefineEnumerationAreaFragment"
        tools:layout="@layout/fragment_define_enumeration_area">

        <action
            android:id="@+id/action_navigate_to_AddHouseholdFragment"
            app:destination="@id/AddHouseholdFragment"/>

        <action
            android:id="@+id/action_navigate_to_AddLocationFragment"
            app:destination="@id/AddLocationFragment"/>

        <action
            android:id="@+id/action_navigate_to_CreateStudyFragment"
            app:destination="@id/CreateStudyFragment"/>

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:popUpTo="@id/ManageConfigurationsFragment"/>
        <action
            android:id="@+id/action_navigate_to_CreateConfigurationFragment"
            app:destination="@id/CreateConfigurationFragment" />
    </fragment>

    <!--CreateStudyFragment-->

    <fragment
        android:id="@+id/CreateStudyFragment"
        android:name="edu.gtri.gpssample.fragments.createstudy.CreateStudyFragment"
        tools:layout="@layout/fragment_create_study">

        <action
            android:id="@+id/action_navigate_to_CreateFieldFragment"
            app:destination="@id/CreateFieldFragment"/>

        <action
            android:id="@+id/action_navigate_to_CreateRuleFragment"
            app:destination="@id/CreateRuleFragment"/>

        <action
            android:id="@+id/action_navigate_to_CreateFilterFragment"
            app:destination="@id/CreateFilterFragment"/>

        <action
            android:id="@+id/action_navigate_to_DefineEnumerationAreaFragment"
            app:destination="@id/DefineEnumerationAreaFragment"/>

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:popUpTo="@id/ManageConfigurationsFragment"/>
        <action
            android:id="@+id/action_navigate_to_CreateConfigurationFragment"
            app:destination="@id/CreateConfigurationFragment" />
    </fragment>

    <!-- ManageEnumerationAreaFragment   -->

    <fragment
        android:id="@+id/ManageEnumerationAreaFragment"
        android:name="edu.gtri.gpssample.fragments.ManageEnumerationArea.ManageEnumerationAreaFragment"
        tools:layout="@layout/fragment_manage_enumeration_area">

        <action
            android:id="@+id/action_navigate_to_CreateTeamFragment"
            app:destination="@id/CreateTeamFragment" />

        <action
            android:id="@+id/action_navigate_to_PerformEnumerationFragment"
            app:destination="@id/PerformEnumerationFragment" />

        <action
            android:id="@+id/action_navigate_to_ManageEnumerationTeamFragment"
            app:destination="@id/ManageEnumerationTeamFragment" />

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:popUpTo="@id/ManageConfigurationsFragment"/>
    </fragment>

    <!--CreateTeamFragment-->

    <fragment
        android:id="@+id/CreateTeamFragment"
        android:name="edu.gtri.gpssample.fragments.create_team.CreateTeamFragment"
        tools:layout="@layout/fragment_create_field">
    </fragment>

    <!--CreateFieldFragment-->

    <fragment
        android:id="@+id/CreateFieldFragment"
        android:name="edu.gtri.gpssample.fragments.createfield.CreateFieldFragment"
        tools:layout="@layout/fragment_create_field">
    </fragment>

    <!--CreateRuleFragment-->

    <fragment
        android:id="@+id/CreateRuleFragment"
        android:name="edu.gtri.gpssample.fragments.createrule.CreateRuleFragment"
        tools:layout="@layout/fragment_create_rule">
    </fragment>

    <!--CreateFilterFragment-->

    <fragment
        android:id="@+id/CreateFilterFragment"
        android:name="edu.gtri.gpssample.fragments.createfilter.CreateFilterFragment"
        tools:layout="@layout/fragment_create_filter">
        <action
            android:id="@+id/action_navigate_to_SelectRuleDialogFragment"
            app:destination="@id/SelectRuleDialogFragment" />
    </fragment>

    <!--ManageSampleFragment-->

    <fragment
        android:id="@+id/ManageSampleFragment"
        android:name="edu.gtri.gpssample.fragments.ManageEnumerationArea.ManageEnumerationAreaFragment"
        tools:layout="@layout/fragment_manage_enumeration_area">

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:popUpTo="@id/ManageConfigurationsFragment"/>
    </fragment>

    <!--SystemStatusFragment-->

    <fragment
        android:id="@+id/SystemStatusFragment"
        android:name="edu.gtri.gpssample.fragments.SystemStatusNotUsed.SystemStatusFragment"
        tools:layout="@layout/fragment_system_status">

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:destination="@id/ManageConfigurationsFragment" />

        <action
            android:id="@+id/action_navigate_to_PerformEnumerationFragment"
            app:destination="@id/PerformEnumerationFragment" />
    </fragment>

    <fragment
        android:id="@+id/ManageEnumerationTeamFragment"
        android:name="edu.gtri.gpssample.fragments.manage_enumeration_team.ManageEnumerationTeamFragment"
        tools:layout="@layout/fragment_manage_enumeration_team">

        <action
            android:id="@+id/action_navigate_to_PerformEnumerationFragment"
            app:destination="@id/PerformEnumerationFragment" />

        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:popUpTo="@id/ManageConfigurationsFragment"/>
    </fragment>

    <fragment
        android:id="@+id/PerformEnumerationFragment"
        android:name="edu.gtri.gpssample.fragments.perform_enumeration.PerformEnumerationFragment"
        tools:layout="@layout/fragment_perform_enumeration">
        <action
            android:id="@+id/action_navigate_to_NetworkConnectionDialogFragment"
            app:destination="@id/NetworkConnectionDialogFragment" />
        <action
            android:id="@+id/action_navigate_to_ManageConfigurationsFragment"
            app:popUpTo="@id/ManageConfigurationsFragment"/>

        <action
            android:id="@+id/action_navigate_to_AddHouseholdFragment"
            app:destination="@id/AddHouseholdFragment"/>

        <action
            android:id="@+id/action_navigate_to_AddLocationFragment"
            app:destination="@id/AddLocationFragment"/>
    </fragment>

    <fragment
        android:id="@+id/AddHouseholdFragment"
        android:name="edu.gtri.gpssample.fragments.add_household.AddHouseholdFragment"
        tools:layout="@layout/fragment_add_household">
    </fragment>

    <fragment
        android:id="@+id/AddLocationFragment"
        android:name="edu.gtri.gpssample.fragments.add_location.AddLocationFragment"
        tools:layout="@layout/fragment_add_location">
    </fragment>
    <fragment
        android:id="@+id/ClientFragment"
        android:name="edu.gtri.gpssample.fragments.client.ClientFragment"
        tools:layout="@layout/fragment_client">
    </fragment>
    <fragment
        android:id="@+id/HotspotFragment"
        android:name="edu.gtri.gpssample.fragments.hotspot.HotspotFragment"
        tools:layout="@layout/fragment_hotspot">
    </fragment>
    <dialog
        android:id="@+id/SelectRuleDialogFragment"
        android:name="edu.gtri.gpssample.dialogs.SelectRuleDialogFragment"
        android:label="dialog_select_rule"
        tools:layout="@layout/dialog_select_rule" />

    <dialog
        android:id="@+id/NetworkConnectionDialogFragment"
        android:name="edu.gtri.gpssample.dialogs.NetworkConnectionDialogFragment"
        android:label="dialog_network_connection"
        tools:layout="@layout/dialog_network_connection" >
        <action
            android:id="@+id/action_navigate_to_HotspotFragment"
            app:destination="@id/HotspotFragment" />
<!--        <action-->
<!--            android:id="@+id/action_FirstFragment_to_ClientFragment"-->
<!--            app:destination="@id/ClientFragment" />-->
<!--        <action-->
<!--            android:id="@+id/action_navigate_to_FirstFragment"-->
<!--            app:destination="@id/FirstFragment" />-->
    </dialog>

</navigation>