<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>Sample16</h:title>
        <model odk:xforms-version="1.0.0">
            <submission orx:auto-delete="default" orx:auto-send="default"/>
            <itext>
                <translation lang="English">
                    <text id="/data/HH_UUID:label">
                        <value>HH UUID</value>
                        <value form="buttonText">Set the HH UUID</value>
                    </text>
                    <text id="/data/HH_UUID:hint">
                        <value>Enter the household UUID</value>
                    </text>
                    <text id="/data/HEAD_OF_HH:label">
                        <value>Head of Household</value>
                    </text>
                    <text id="/data/HEAD_OF_HH:hint">
                        <value>Enter the name of the head of household</value>
                    </text>
                </translation>
            </itext>
            <instance>
                <data id="Sample16" version="1687533180">
                    <HH_UUID/>
                    <HEAD_OF_HH/>
                    <meta>
                        <instanceID/>
                    </meta>
                </data>
            </instance>
            <bind nodeset="/data/HH_UUID" required="true()" type="string"/>
            <bind nodeset="/data/HEAD_OF_HH" required="true()" type="string"/>
            <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
        </model>
    </h:head>
    <h:body>
        <input appearance="ex:edu.gtri.gpssample.odk" ref="/data/HH_UUID">
            <label ref="jr:itext('/data/HH_UUID:label')"/>
            <hint ref="jr:itext('/data/HH_UUID:hint')"/>
        </input>
        <input ref="/data/HEAD_OF_HH">
            <label ref="jr:itext('/data/HEAD_OF_HH:label')"/>
            <hint ref="jr:itext('/data/HEAD_OF_HH:hint')"/>
        </input>
    </h:body>
</h:html>
